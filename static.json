{
  "root": "build/",
  "https_only": true,
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=7776000",
      "Access-Control-Allow-Origin": "*"
    },
"proxies": {
    "/api/": {
    "origin": "https://${API_APP_NAME}.herokuapp.com/"
    }
  }
}
}